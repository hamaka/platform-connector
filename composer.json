{
  "name": "webservices-nl/platform-connector",
  "description": "platform connector generator for Webservices.nl",
  "keywords": [
    "soap",
    "soapclient",
    "webservicesNl",
    "webservices.nl"
  ],
  "config": {
    "gitlab-domains": [
      "gitlab.fdmg.org"
    ],
    "preferred-install": "dist"
  },
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "WebservicesNL",
      "email": "tech@webservices.nl",
      "homepage": "http://www.webservices.nl"
    }
  ],
  "prefer-stable": true,
  "require": {
    "php": "^5.6 || ^7 || ^8",
    "ext-soap": "*",
    "ext-xml": "*",
    "ext-dom": "*",
    "guzzlehttp/guzzle": "^6 || ^7.0.1",

    "webservices-nl/common": "ss5 as dev-master",
    "webservices-nl/utils": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "^5",
    "mockery/mockery": "^0.9",

    "league/factory-muffin": "^2",
    "friendsofphp/php-cs-fixer": "^2 || ^3",
    "sensiolabs/security-checker": "^5",
    "squizlabs/php_codesniffer": "^2"
  },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/carelevers/common"
        }
    
    ],
  "autoload": {
    "psr-4": {
      "WebservicesNl\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WebservicesNl\\Test\\": "test/"
    }
  }
}
